/* pages/category/detail/index.wxss */
.clearfix {
	zoom: 1;
	clear: both;
} 
.clearfix:after {
	display: block;
	clear: both;
	content: '';
	visibility: hidden;
	height: 0;
}
.project-head-banners,
.project-head-banners-swiper {
	width: 750rpx;
	height: 755rpx;
}
.slide-image {
	width: 750rpx;
	height: 755rpx;
}
.project-detail {
    background: #fff;
}

.project-detail .project-head {
    display: flex;
    flex-direction: column;
}

.project-detail .project-head image {
    width: 750rpx;
    height: 842rpx;
}

.project-head .top-view {
    margin: 20rpx 30rpx 0 30rpx;
    display: flex;
    justify-content: space-between;
}

.money-view {
    font-size: 40rpx;
    font-weight: 400;
    color: #FD9E00;
}

.project-detail-h1 {
    font-size: 34rpx;
    font-weight: 500;
    color: #000000;
}

.project-detail-h2 {
    font-size: 24rpx;
    font-weight: 400;
    color: #B8B8B8;
}

.share-view {
    font-size: 22rpx;
    color: #A6A6A6;
    font-weight: 400;
}

/** 地址 */

.address-view {
    padding: 20rpx 36rpx;
    font-size: 24rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #565656;
}

.address-view .address-title {
    flex: 1;
    color: #A6A6A6;
}

.address-view .address-right {
    flex: 4;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

/** 推荐 */

.recommend-view {
    border-top: 16rpx solid #F6F7FB;
}

.recommend-view .recommend-title {
    padding: 10px 0;
    font-size: 32rpx;
    font-weight: 400;
    margin-left: 30rpx;
}

/** 纵向 */

.project-list {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    margin: 0 30rpx 0 30rpx;
}

.project-list .item {
    margin-bottom: 28rpx;
}

/** 底部按钮 */

.project-detail-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 100rpx;
    display: flex;
    align-items: center;
    background: white;
    border-top: 1rpx solid #E7E7E7;
}

.project-detail-bottom .payment {
    color: #fff;
    font-size: 28rpx;
    height: 62rpx;
    width: 398rpx;
    line-height: 62rpx;
    text-align: center;
    border-radius: 62rpx;
    background: #FBA953;
    position: absolute;
    right: 30rpx;
}

.project-detail-bottom .icon_title_view {
    color: #9E9E9E;
    font-size: 24rpx;
    text-align: center;
    position: absolute;
    left: 90rpx;
    display: flex;
    align-items: center;
}

.icon_title {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.icon_title image {
    width: 36rpx;
    height: 36rpx;
}
.shadow_view {
	height: 1317rpx;
	padding: 27rpx 31rpx 280rpx 32rpx;
}
.shadow_view_goods {
	display: flex;
	justify-content: space-between;
	height: 171rpx;
	margin-bottom: 35rpx;
}
.shadow_view_goods_img {
	width: 189rpx;
	height: 171rpx;
	margin-right: 22rpx;
}
.shadow_view_goods_main {
	width: 493rpx;
	padding-top: 68rpx;
}
.shadow_view_goods_main_price {
	color: #FFA030;
	font-size: 46rpx;
	height: 65rpx;
	line-height: 65rpx;
	margin-bottom: 3rpx;
}
.shadow_view_goods_main_price text {
	font-size: 22rpx;
}
.shadow_view_goods_main_tip {
	height: 33rpx;
	line-height: 33rpx;
	font-size: 24rpx;
	font-family: PingFangSC-Regular,PingFang SC;
	font-weight: 400;
	color: #A6A6A6;
}
.shadow_view_section {
	margin-bottom: 29rpx;
}
.shadow_view_section_title {
	height: 42rpx;
	display: flex;
	justify-content: space-between;
}
.shadow_view_section_title_label {
	height: 42rpx;
	font-size: 30rpx;
	font-family: PingFangSC-Regular,PingFang SC;
	font-weight: 400;
	color: rgba(0,0,0,1);
	line-height: 42rpx;
}
.shadow_view_section_tips {
	height: 33rpx;
	font-size: 24rpx;
	font-family: PingFangSC-Regular,PingFang SC;
	font-weight: 400;
	color: rgba(253,158,0,1);
	line-height: 33rpx;
	margin: 19rpx 0 10rpx 0;
}
.shadow_view_section_operates {
	display: flex;
	justify-content: space-between;
}
.shadow_view_section_operates_item {
	width: 329rpx;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
	background: #EBEBEB;
	border-radius: 30rpx;
	-moz-border-radius: 30rpx;
	-webkit-border-radius: 30rpx;
	font-size: 24rpx;
	font-family: PingFangSC-Regular,PingFang SC;
	font-weight: 400;
	color: #000;
}
.shadow_view_section_size,
.shadow_view_section_special {
	margin-top: 17rpx;
}
.shadow_view_section_size_item,
.shadow_view_section_special_item {
	float: left;
	height: 60rpx;
	line-height: 60rpx;
	padding: 0 38rpx;
	background: #EBEBEB;
	border-radius: 30rpx;
	-moz-border-radius: 30rpx;
	-webkit-border-radius: 30rpx;
	font-size: 24rpx;
	font-family: PingFangSC-Regular,PingFang SC;
	font-weight: 400;
	color: #000;
	margin-right: 27rpx;
	margin-bottom: 17rpx;
}
.shadow_view_section_service {
	margin-top: 17rpx;
	margin-bottom: 28rpx;
}
.shadow_view_section_service_item {
	margin-bottom: 24rpx;
}
.shadow_view_section_service_item_tip {
	height: 33rpx;
	line-height: 33rpx;
	font-size: 24rpx;
	font-family: PingFangSC-Regular,PingFang SC;
	font-weight: 400;	
	color: #000;
	margin-bottom: 20rpx;
}
.shadow_view_section_service_item_tip image {
	width: 33rpx;
	height: 33rpx;
	margin-right: 15rpx;
	vertical-align: middle;
}
.shadow_view_section_service_item_main {
	font-size: 24rpx;
	font-family: PingFangSC-Regular,PingFang SC;
	font-weight: 400;
	color: #000;
}
.shadow_view_section_service_item_main_content {
	float: left;
	margin-bottom: 15rpx;
	width: 329rpx;
	text-align: center;
	background: rgba(235,235,235,1);
	border-radius: 42rpx;
	-moz-border-radius: 42rpx;
	-webkit-border-radius: 42rpx;
	padding: 14rpx 0 12rpx 0;
}
.shadow_view_section_service_item_main_content:nth-child(2n-1) {
	margin-right: 29rpx;
}
.shadow_view_section_service_item_main_content_warranty {
	height: 33rpx;
	font-size: 24rpx;
	font-family: PingFangSC-Regular,PingFang SC;
	font-weight: 400;
	color: rgba(0,0,0,1);
	line-height: 33rpx;
}
.shadow_view_section_service_item_main_content_desc {
	height: 25rpx;
	font-size: 18rpx;
	font-family: PingFangSC-Regular,PingFang SC;
	font-weight: 400;
	color: rgba(255,0,0,1);
	line-height: 25rpx;
}
.shadow_view_footer {
	border-top: 1rpx solid #E7E7E7;
	position: fixed;
	left: 0;
	bottom: 0;
	width: 100vw;
	padding-top: 21rpx;
	padding-bottom: 15rpx;
	background: #fff;
	z-index: 999;
}
.shadow_view_footer_operate {
	width: 689rpx;
	margin: 0 auto;
	height: 62rpx;
	line-height: 62rpx;
	text-align: center;
	background: rgba(255,160,48,1);
	border-radius: 32rpx;
	-moz-border-radius: 32rpx;
	-webkit-border-radius: 32rpx;
	font-size: 28rpx;
	color: #fff;
	font-family: ingFangSC-Regular,PingFang SC;
	font-weight: 400;
}
.active {
	background: #FFA030;
	color: #fff;
}
.shadow_view_section_num {
	width: 210rpx;
	height: 56rpx;
	line-height: 56rpx;
	border-radius: 8rpx;
	display: inline-flex;
}
.shadow_view_section_num_min {
	width: 56rpx;
	text-align: center;
	background: #EEEFF2;
	color: #C0C0C0;
	font-size: 24rpx;
	border-bottom-left-radius: 8rpx;
	border-top-left-radius: 8rpx;
}
.weui-input {
	width: 98rpx;
	background: #F6F7FB;
	text-align: center;
	font-size: 24rpx;
	font-family: PingFangSC-Regular,PingFang SC;
	font-weight: 400;
	color: #000;
}
.shadow_view_section_num_add {
	width: 56rpx;
	text-align: center;
	background: #EEEFF2;
	font-size: 24rpx;
	color: #000;
	border-bottom-right-radius: 8rpx;
	border-top-right-radius: 8rpx;
}