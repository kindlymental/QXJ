/* pages/category/service_detail/index.wxss */

.project-head-banners,
.project-head-banners-swiper {
	width: 750rpx;
	height: 755rpx;
}
.slide-image {
	width: 750rpx;
	height: 755rpx;
}
.project-detail {
  background: #fff;
}

.project-detail .project-head {
  display: flex;
  flex-direction: column;
}

.project-detail .project-head image {
  width: 750rpx;
  height: 842rpx;
}

.project-head .top-view {
  margin: 20rpx 30rpx 0 30rpx;
  display: flex;
  justify-content: space-between;
}

.money-view {
  font-size: 40rpx;
  font-weight: 400;
  color: #FD9E00;
}

.project-detail-h1 {
  font-size: 34rpx;
  font-weight: 500;
  color: #000000;
}

.project-detail-h2 {
  font-size: 24rpx;
  font-weight: 400;
  color: #B8B8B8;
}

.share-view {
  font-size: 22rpx;
  color: #A6A6A6;
  font-weight: 400;
}

/** 地址 */
.address-view {
  padding: 34rpx 36rpx 26rpx 36rpx;
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #565656;
}

.address-view .address-title {
  flex: 1;
  color: #A6A6A6;
}
.address-view .address-right {
  flex: 4;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

/** 推荐 */
.recommend-view {
  border-top: 16rpx solid #F6F7FB;
}

.recommend-view .recommend-title {
  padding: 10px 0;
  font-size: 32rpx;
  font-weight: 400;
  margin-left: 30rpx;
}

/** 纵向 */
.project-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin: 0 30rpx 0 30rpx;
}

.project-list .item {
  margin-bottom: 28rpx;
}

/** 底部按钮 */
.project-detail-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  display: flex;
  align-items: center;
  background: white;
  border-top: 1rpx solid #E7E7E7;
}

.project-detail-bottom .payment {
  color: #fff;
  font-size: 28rpx;
  height: 62rpx;
  width: 494rpx;
  line-height: 62rpx;
  text-align: center;
  border-radius: 62rpx;
  background: #FBA953;
  position: absolute;
  right: 30rpx;
}

.project-detail-bottom .icon_title_view {
  color: #9E9E9E;
  font-size: 24rpx;
  text-align: center;
  position: absolute;
  left: 90rpx;
  display: flex;
  align-items: center;
}

.icon_title {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.icon_title image {
  width: 36rpx;
  height: 36rpx;
}

/** 时间选择器 */
.datetime-picker-view {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}

