.member_list {
	padding: 33rpx 47rpx 33rpx 46rpx;
	background: #F6F7FB;
}
.member_list_item {
	width: 657rpx;
	height: 315rpx;
	position: relative;
	margin-bottom: 22rpx;
}
.member_list_item_bg {
	width: 100%;
	height: 100%;
}
.member_list_item_main {
	position: absolute;
	top: 96rpx;
	width: 504rpx;
	left: 50%;
	transform: translateX(-50%);
	text-align: center;
}
.member_list_item_main_title {
	height: 53rpx;
	font-size: 38rpx;
	font-family: PingFangSC-Medium,PingFang SC;
	font-weight: 500;
	color: rgba(255,255,255,1);
	line-height: 53rpx;
	margin-bottom: 15rpx;
}
.member_list_item_main_desc {
	height: 66rpx;
	font-size: 24rpx;
	font-family: PingFangSC-Regular,PingFang SC;
	font-weight: 400;
	color: rgba(255,255,255,1);
	line-height: 33rpx;
}